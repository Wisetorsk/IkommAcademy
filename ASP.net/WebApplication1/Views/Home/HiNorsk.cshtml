@{
}
<h1>Heisann!</h1>
<div>
    <p>
        This is @ViewData["Country"]<br />
        The time in @ViewData["Country"] now is: @DateTime.UtcNow.AddHours((int)ViewData["timeOffset"])
    </p>
</div>
@{ 
    var persons = (List<string>)ViewData["people"];
}
<ul>
    
        @if (!(ViewData["people"] is null))
        {
            foreach (var person in persons)
            {
            <text><li>@person</li></text>
                }
            }
        
</ul>